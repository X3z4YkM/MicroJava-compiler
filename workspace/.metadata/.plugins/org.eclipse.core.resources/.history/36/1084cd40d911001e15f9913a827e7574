program test_A_successful
  int b[][];
  int mat3 [][];
  int niz[];
{
    void main()int s;  {
     	b = new int[3][2];
     	mat3 = new int[1][2];
     	niz = new int[40];
     	s=2;
     	b[0][1] = 3;
     	b[0][2] = s; 
     	b[s][1] = 100;
     	s++; 
     	b[s][s]++;
     	b[2] = niz;
     	niz[2] = 2;
     	niz[1] = 0;
     	b[niz[2]][niz[1]] = niz[3];
     	mat3[b[mat3[s][niz[2]]][2]][1] = 3;
     	
     	s = b[1][2];
     	s = b[niz[2]][niz[1]];
     	niz[2] = b[niz[2]][niz[1]];
     	b[niz[2]][niz[1]] = b[niz[2]][niz[1]];
     	
     	s = mat3[b[mat3[s][niz[2]]][2]][1];
     	niz[mat3[s][2]] = mat3[b[mat3[s][niz[2]]][2]][1]
		return;
    }
}